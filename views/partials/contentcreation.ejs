    .animated-background {

      background: linear-gradient(45deg, #ff5733, #ff33d3, #33a4ff, #33ff6d);
      background-size: 400% 400%;
      animation: gradientBG 10s ease infinite;
    }

    @keyframes gradientBG {
      0% {
        background-position: 0% 50%;
      }
      50% {
        background-position: 100% 50%;
      }
      100% {
        background-position: 0% 50%;
      }
    }

    .content {
      text-align: center;
      position: relative;
      z-index: 1;
    }
  </style>
